{
    "family": "ecs-deploy-basic",
    "executionRoleArn": "arn:aws:iam::322725876573:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "basic-container",
            "image": "322725876573.dkr.ecr.eu-west-1.amazonaws.com/ecs-deploy",
            "cpu": 512,
            "portMappings": [
              {
                "hostPort": 8080,
                "protocol": "tcp",
                "containerPort": 8080
              }
            ],
            "memory": 1024,
            "essential": true,
            "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-group": "/ecs/ecs-deploy",
                "awslogs-region": "eu-west-1",
                "awslogs-stream-prefix": "ecs"
              }
            }
        }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048"
  }